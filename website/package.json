{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "make prebuild && NODE_OPTIONS=--max-old-space-size=8192 docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "ci": "yarn lint && yarn prettier:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,md}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,md}\""
  },
  "dependencies": {
    "@docusaurus/core": "2.1.0",
    "@docusaurus/plugin-google-analytics": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "clsx": "^1.1.1",
    "docusaurus-plugin-internaldocs-fb": "1.5.0",
    "plotly.js": "^2.8.1",
    "react": "^17.0.2",
    "react-dom": "^16.8.4",
    "react-loadable": "^5.5.0",
    "react-plotly.js": "^2.5.1",
    "rehype-katex": "^3.0.0",
    "remark-math": "^2.0.1",
    "trim": "^0.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "^2.0.2",
    "react-docgen": "^5.2.1",
    "stylelint": "^13.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "ansi-html": "0.0.8",
    "shelljs": "^0.8.5",
    "trim": "^0.0.3"
  }
}
